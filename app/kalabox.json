{
  "appName": "panth",
  "appPlugins": [
    "kalabox-plugin-git",
    "kalabox-plugin-pantheon"
  ],
  "shareIgnores": [
    "sites/default/files/*",
    "files",
    ".git"
  ],
  "pluginConf": {
    "kalabox-plugin-pantheon": {
      "framework": "drupal",
      "solr": false,
      "redis": false,
      "php": "5.3.29",
      "drush-version": "drush7"
    },
    "kalabox-plugin-git": {
      "git-username": "Mike Pirog",
      "git-email": "mike@kalamuna.com"
    }
  },
  "appComponents": {
    "appserver": {
      "image": {
        "name": "pantheon-appserver",
        "srcRoot": ":appRoot:"
      },
      "proxy": [
        {
          "port": "80/tcp",
          "default": true
        },
        {
          "port": "443/tcp",
          "default": true,
          "secure": true
        }
      ]
    },
    "db": {
      "image": {
        "name": "mariadb",
        "srcRoot": ":appRoot:"
      }
    },
    "solr": {
      "image": {
        "name": "pantheon-solr",
        "srcRoot": ":appRoot:"
      }
    },
    "redis": {
      "image": {
        "name": "pantheon-redis",
        "srcRoot": ":appRoot:"
      }
    }
  }
}
