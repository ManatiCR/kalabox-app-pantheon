{
  "appName": "panth",
  "appPlugins": [
    "kalabox-plugin-php",
    "kalabox-plugin-git",
    "kalabox-plugin-pantheon-env"
  ],
  "shareIgnores": [
    "sites/default/files/*",
    "files",
    ".git"
  ],
  "pluginConf": {
    "kalabox-plugin-php": {
      "php-version": "5.3.29"
    },
    "kalabox-plugin-drush": {
      "drush-version": "6"
    },
    "kalabox-plugin-git": {
      "git-username": "Mike Pirog",
      "git-email": "mike@kalamuna.com"
    }
  },
  "appComponents": {
    "appserver": {
      "image": {
        "name": "pantheon-appserver",
        "srcRoot": ":appRoot:"
      },
      "proxy": [
        {
          "port": "80/tcp",
          "default": true
        }
      ]
    },
    "db": {
      "image": {
        "name": "mariadb",
        "srcRoot": ":appRoot:"
      }
    }
  }
}
